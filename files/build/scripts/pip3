#!/bin/bash

. /usr/local/share/buildinfo/scripts/buildinfo_base.sh
REAL_COMMAND=/usr/bin/pip3
[ -x /usr/local/bin/pip3 ] && REAL_COMMAND=/usr/local/bin/pip3

if [ ! -x "$REAL_COMMAND" ]; then
    echo "The command pip3 not found" 1>&2
    exit 1
fi

ENABLE_VERSION_CONTROL_PY=$ENABLE_VERSION_CONTROL_PY2 REAL_COMMAND=$REAL_COMMAND run_pip_command "$@"

